// training sri
var m  map[int]string
m2 := make(map[int]string)

m2[1] = "abc"
m2[2] = "cde"
// you can give any index/key - not like in array/slice where serially index gets assigned
m2[10] = "cde"

fmt.Println(m2[1])
fmt.Println(m2[2])
fmt.Println(m2[10])

for key, value := range m2 {
fmt.Println(key, value)
}

v, ok := m[1]
if ok {
fmt.Println(v)
} else {
fmt.Println("Key does not exist")
}

